<?xml version="1.0" encoding="UTF-8"?><environment>

	<fwrules>
		<in access="allow" direction="in" from-ips="all" name="SSH" protocols="tcp" to-ports="22"/>
	</fwrules>
	
	<models>
		<model imageId="RHEL-6.3-minimal" instanceType="t1.micro" keyPairName="id_rsa" name="RHEL-6.3-minimal" region="qemu:///system">
			<network-management enable="true" method="ssh" port="22"/>
			<disk-management diskDevicesSelector="//disk"/>
			<fwrules>
				<fwrule access="allow" direction="in" from-ips="all" protocols="tcp" to-ports="8080"/>
				<fwrule access="allow" direction="in" from-ips="10.10.0.0/16" protocols="tcp" to-ports="678"/>
				<fwrule herit="./environment/fwrules/in[@name='SSH']"/>
			</fwrules>
			<disks>
				<disk deleteOnTermination="true" device="/dev/vda" rootDevice="true"/>
			</disks>
		</model>
	</models>
	
	<instances>
		
		<instance herit="./environment/models/model[@name='RHEL-6.3-minimal']" name="srv01">
			<network>
				<interface device="eth0"/>
			</network>
		</instance>
		
