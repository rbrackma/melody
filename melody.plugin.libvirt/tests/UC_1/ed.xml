<?xml version="1.0" encoding="UTF-8"?><environment>

	<fwrules>
		<in access="allow" from="all" name="SSH" ports="22" protocols="tcp"/>
	</fwrules>
	
	<models>
		<model imageId="RHEL-6.3-minimal" instanceType="t1.micro" keyPairName="id_rsa" name="RHEL-6.3-minimal" region="qemu:///system">
			<tags>
				<tag name="MGNT" value="ssh"/>
				<tag name="SSH.PORT" value="22"/>
			</tags>
			<fwrules>
				<in>
					<fwrule access="allow" from="all" ports="8080" protocols="tcp"/>
					<fwrule herit="./environment/global_fwrules/in[@name='SSH']"/>
				</in>
			</fwrules>
			<devices>
				<disk deleteOnTermination="true" device="/dev/sda1" rootDevice="true"/>
			</devices>
		</model>
	</models>
	
	<instances>
		<instance herit="./environment/models/model[@name='RHEL-6.3-minimal']" name="srv01"/>
		<instance herit="./environment/models/model[@name='RHEL-6.3-minimal']" name="srv02"/>
		<instance herit="./environment/models/model[@name='RHEL-6.3-minimal']" name="srv03"/>
	</instances>
	
</environment>