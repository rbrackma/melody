<?xml version="1.0" encoding="UTF-8"?><environment>

	<serverAWS>
	
		<instance AWS_instance_Id="i-aee53ce5" herit="./environment/models/model[@name='RHEL6.3-jboss-zip']" name="test-zip"/>

		<instance AWS_instance_Id="i-442af30f" herit="./environment/models/model[@name='RHEL6.3-jboss-rpm']" name="test-rpm"/>

	</serverAWS>
	
	<models>
		<model herit="./environment/models/model[@name='base']" imageId="ami-f3454587" name="RHEL6.3-jboss-zip"/>

		<model herit="./environment/models/model[@name='base']" imageId="ami-c9b5b4bd" name="RHEL6.3-jboss-rpm">
			<fwrules>
				<in>
					<fwrule access="allow" from="all" ports="80,442,443" protocols="tcp"/>
				</in>
			</fwrules>
		</model>

		<model instanceType="t1.micro" keyPairName="id_rsa_aws" name="base" region="eu-west-1">
			<tags>
				<tag name="MGNT" value="ssh"/>
				<tag name="SSH.PORT" value="22"/>
			</tags>
			<fwrules>
				<in>
					<fwrule herit="./environment/fwrules/fwrule[@name='SSH']"/>
					<fwrule access="allow" from="all" ports="8080,8442,8443" protocols="tcp"/>
				</in>
			</fwrules>
			<devices>
				<disk deleteOnTermination="true" device="/dev/sda1" rootDevice="true"/>
			</devices>
		</model>
	</models>

	<fwrules>
		<fwrule access="allow" from="all" name="SSH" ports="22" protocols="tcp"/>
		<fwrule access="allow" from="all" name="RDP" ports="3389" protocols="tcp"/>
	</fwrules>
	
</environment>