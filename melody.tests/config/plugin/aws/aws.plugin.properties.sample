#####################
### Mandatory Melody plugin configuration directives
#####################

### the class which will load this configuration
plugin.configuration.canonicalclassname=com.wat.melody.plugin.aws.common.AwsPlugInConfiguration


#####################
### AWS credentials
#####################

### the AWS access key
accessKey=<replace me with your AWS Acces Key>

### the AWS access key
secretKey=<replace it with your AWS Secret Key>


#####################
### AWS Client Configuration
#####################

### Socket settings
#aws.conn.socket.connect.timeout=
#aws.conn.socket.read.timeout=
#aws.conn.socket.max.error.retry=
#aws.conn.socket.buffer.size.send=
#aws.conn.socket.buffer.size.receive=
#aws.conn.max.pool.size=

### L7 setting
#aws.conn.http.protocol=
aws.conn.http.useragent=Melody_GPL_AutomationTool_madeBy_GuillaumeCornet

### Proxy settings
#aws.conn.proxy.host=
#aws.conn.proxy.port=
#aws.conn.proxy.username=
#aws.conn.proxy.password=

### Client Side encryption settings
# can be 'file' or 'metadata'; default is 'metedata'
#aws.encryption.clientside.storagemode=
# the canonical name of a security Provider
# set it to 'org.bouncycastle.jce.provider.BouncyCastleProvider' to use Bouncy Castle; default provider is JCE
#aws.encryption.clientside.provider=
